{{{script}}}

export default {
  key: '{{key}}',
  codesandboxes: [
    {{#each codesandboxes}}
    {
      port: '{{this.port}}',
      url: '{{{this.url}}}',
    },
    {{/each}}
  ],
  ports: {
    preview: {
      script: App,
    },
    {{#each ports}}
    {{@key}}: {
      {{#each this}}
      {{#if @this}}
      {{@key}}: {{{json @this}}},
      {{else}}
      {{@key}}: "",
      {{/if}}
      {{/each}}
    },
    {{/each}}
  }
}